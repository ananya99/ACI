<mujoco model="scene">
    <compiler assetdir="assets" angle="degree" coordinate="local" inertiafromgeom="true" />
    <option density="1000" integrator="implicitfast" timestep="0.02" viscosity="1.8e-5" />
    <visual>
        <map znear="0.001" zfar="2" />
        <!-- offsamples=0 only calls one gpu render, defaults to 4. -->
        <quality offsamples="0" />
        <scale frustum="40" />
    </visual>
    <asset>
        <texture type="skybox" builtin="gradient" width="100" height="100" rgb1="0 0 0" rgb2="0 0 0" />
    </asset>
    <worldbody></worldbody>
</mujoco>